{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
      "object-curly-spacing": ["warn", "never"],
      "func_names": "off",
      "space-before-function-paren": ["error", "never"],
      "max-len": ["error", 120, 4],
      "no-unused-vars": ["error", {"argsIgnorePattern": "next"}],
      "import/no-extraneous-dependencies": ["error", {"devDependencies": false}]
    }
}
